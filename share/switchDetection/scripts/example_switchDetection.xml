<application>
    <name>example switchDetection App</name>

    <module>
        <name>yarpdev</name>
        <parameters>--context sensors --from asus-xtion.ini</parameters>
        <node>localhost</node>
    </module>

    <module>
        <name>switchDetection</name>
        <parameters>--RGBDLocal /xtion</parameters>
        <node>localhost</node>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /haarDetection/yarpview/img:i</parameters>
        <node>localhost</node>
    </module>
    <connection>
        <from>/haarDetection/img:o</from>
        <to>/haarDetection/yarpview/img:i</to>
    </connection>

</application>
